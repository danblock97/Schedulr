<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>supabase.oauth.callback</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>schedulr</string>
				<string>$(SUPABASE_OAUTH_CALLBACK_SCHEME)</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLName</key>
			<string>google.reversed.client.id</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>$(GOOGLE_REVERSED_CLIENT_ID)</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>GIDClientID</key>
	<string>$(GOOGLE_CLIENT_ID)</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Schedulr needs calendar access so you can share availability with your groups.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Schedulr uses your microphone so you can talk to Scheduly, your AI scheduling assistant.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>We use your photo to set a profile avatar.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Schedulr uses speech recognition to convert your voice into text when chatting with Scheduly.</string>
	<key>OPENAI_API_KEY</key>
	<string>$(OPENAI_API_KEY)</string>
	<key>REVENUECAT_API_KEY</key>
	<string>$(REVENUECAT_API_KEY)</string>
	<key>REVENUECAT_PRO_ENTITLEMENT</key>
	<string>$(REVENUECAT_PRO_ENTITLEMENT)</string>
	<key>REVERSED_CLIENT_ID</key>
	<string>$(GOOGLE_REVERSED_CLIENT_ID)</string>
	<key>SUPABASE_ANON_KEY</key>
	<string>$(SUPABASE_ANON_KEY)</string>
	<key>SUPABASE_OAUTH_CALLBACK_SCHEME</key>
	<string>$(SUPABASE_OAUTH_CALLBACK_SCHEME)</string>
	<key>SUPABASE_SERVICE_ROLE_KEY</key>
	<string>$(SUPABASE_SERVICE_ROLE_KEY)</string>
	<key>SUPABASE_URL</key>
	<string>$(SUPABASE_URL)</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiresFullScreen</key>
	<true/>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
</dict>
</plist>
